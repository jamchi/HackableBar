    "sass-loader": "^13.2.0",
    "style-loader": "^1.3.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "@google-cloud/functions-framework": "^1.5.1",
    "@octokit/rest": "^17.6.0",
    "core-js": "^3.6.5",
    "firebase": "^7.14.2"
  }
